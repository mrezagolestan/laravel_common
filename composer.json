{
    "name": "mrezagolestan/laravel_common",
    "description": "Piod Laravel Common Package",
    "type": "project",
    "keywords": [
        "piod",
        "laravel",
        "common"
    ],
    "require": {
        "php-amqplib/php-amqplib": "^3.2",
        "sentry/sentry-laravel": "^2.9",
        "predis/predis": "^2.0",
        "fruitcake/laravel-cors": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Reza Golestan",
            "email": "it@pio.ir"
        }
    ],
    "autoload": {
        "psr-4": {
            "Piod\\LaravelCommon\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Piod\\LaravelCommon\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },

    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Piod\\LaravelCommon\\LaravelCommonServiceProvider"
            ]
        }
    }
}
